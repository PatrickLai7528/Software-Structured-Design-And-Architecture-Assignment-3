<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="model.AdministratorEntity" table="administrator" schema="librarydb">
        <id name="aid">
            <column name="aid" sql-type="int(11)"/>
        </id>
        <property name="aname">
            <column name="aname" sql-type="varchar(255)"/>
        </property>
    </class>
</hibernate-mapping>