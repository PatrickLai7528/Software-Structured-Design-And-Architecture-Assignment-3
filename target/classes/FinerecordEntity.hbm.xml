<?xml version='1.0' encoding='utf-8'?>
<!DOCTYPE hibernate-mapping PUBLIC
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>

    <class name="model.FinerecordEntity" table="finerecord" schema="librarydb">
        <id name="fid">
            <column name="fid" sql-type="int(11)"/>
        </id>
        <property name="uid">
            <column name="uid" sql-type="int(11)"/>
        </property>
        <property name="bid">
            <column name="bid" sql-type="int(11)"/>
        </property>
        <property name="latedate">
            <column name="latedate" sql-type="int(11)"/>
        </property>
        <property name="finetotal">
            <column name="finetotal" sql-type="double" precision="-1"/>
        </property>
    </class>
</hibernate-mapping>